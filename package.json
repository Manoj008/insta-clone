    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "prop-types": "^15.7.2",
    "tailwindcss": "^2.1.2"
  }
