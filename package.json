{
  "homepage": "https://manoj008.github.io/insta-clone",
  "name": "insta-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "date-fns": "^2.21.3",
    "firebase": "^8.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^2.2.0",
    "react-router-dom": "^5.2.0",
